#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,num,flag=0;
        cin>>n>>num;
        int arr[n];
        for(int i=0;i<n;i++)
            cin>>arr[i];
        for(int i=0;i<n;i++)
        {
            if(arr[i]==num)
            {
                cout<<"found at location- "<<i+1;
                flag=1;
                break;
            }
        }
        if(flag==0)
        {
            cout<<"Element is not present";
        }
        cout<<endl;
    }
   return 0;
}
